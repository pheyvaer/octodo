{
  "manifest_version": 2,
  "name": "todo-creator",
  "description": "Creates markdown todos",
  "version": "0.0.1",
  "icons": {
    "64": "icons/octopus.png"
  },
  "browser_action": {
    "default_icon": {
      "64": "icons/octopus.png"
    },
    "default_popup": "browserAction/index.html",
    "default_title": "todo-creator"
  },
  "options_ui": {
    "page": "options/index.html"
  },
  "permissions": [
    "storage",
    "clipboardWrite",
    "activeTab"
  ]
}
