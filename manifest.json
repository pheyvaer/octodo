{
  "manifest_version": 2,
  "name": "todo-creator",
  "description": "Creates markdown todos",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.github.com/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "browserAction/index.html",
    "default_title": "todo-creator"
  },
  "options_ui": {
    "page": "options/index.html"
  },
  "permissions": [
    "storage",
    "clipboardWrite"
  ]
}
